<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context  
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        <beans>
    <bean id="bmw" class="com.general.verified.web.spring.ioc.inject.setter.Bmw" />
    <bean id="benz" class="com.general.verified.web.spring.ioc.inject.setter.Benz" />
    <bean id="driver" class="com.general.verified.web.spring.ioc.inject.setter.Driver">
        <property name="car">
			<ref bean="bmw" />
		</property>
    </bean>
</beans>
        
       <!-- <context:annotation-config/> -->
</beans>