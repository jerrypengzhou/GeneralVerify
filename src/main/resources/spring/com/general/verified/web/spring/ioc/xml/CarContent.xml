<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context  
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">
        <beans>
    <bean id="BMW" class="com.general.dev.verified.web.spring.ioc.test.BMW" />
    <bean id="car" class="com.general.dev.verified.web.spring.ioc.test.CarImpl" />
    <bean id="driver" class="com.general.dev.verified.web.spring.ioc.test.Driver">
        <constructor-arg value="Tom" />
        <property name="car" ref="BMW" />
    </bean>
</beans>
        
       <!-- <context:annotation-config/> -->
</beans>